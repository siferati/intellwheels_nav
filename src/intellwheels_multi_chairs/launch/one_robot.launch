<launch>
    <arg name="robot_name"/>
    <arg name="init_pose"/>

    <node name="spawn_model" pkg="gazebo_ros" type="spawn_model"
     args="$(arg init_pose) -urdf -param /robot_description -model $(arg robot_name)"
     respawn="false" output="screen" />


    <!-- Problem here !-->
